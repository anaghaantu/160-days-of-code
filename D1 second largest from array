# GET SECOND LARGEST FROM AN ARRAY
N = int(input("Enter the size of array:"))
arr = [ ]
for i in range(N):
    arr.append(int(input("enter the value:")))
print(arr)
def secondlargest(arr):
    arr = sorted(set(arr))
    if len(arr) < 2:
        return -1
    else:
        return arr[-2]
secondlargest(arr)
print("Second largest element:", secondlargest(arr))

