# GET SECOND LARGEST FROM AN ARRAY
N = int(input("Enter the size of array:"))
arr = [ ]
for i in range(N):
    arr.append(int(input("enter the value:")))
print(arr)
def secondlargest(arr):
    arr.sort()
    print(arr)
    for i in range(N-2,-1,-1):
        if arr[i]!=arr[N-1]:
           return arr[i]
        else:
            return -1
secondlargest(arr)
