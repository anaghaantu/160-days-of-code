n = int(input("enter the size of array:"))
n
arr = []
for i in range(n):
    arr.append(int(input("enter the array elements:")))
print(arr)
d = int(input("enter the d value:"))
d
def shifting(arr,d):
    for i in range(d):
        first = arr[0]
        for j in range(n-1):
            arr[j] = arr[j+1]
        arr[n-1]=first
    print(arr)
shifting(arr,d)      
    
